<div>
  <%= render 'users/user', user: current_user %>
  <ul>
    <% @posts.includes(:user).each do |post| %>
      <%= render 'post', post: post %>
      <%= render 'comment', comments: post.comments %>
    <% end %>
  </ul>
  <div class="see_all_post text-center">
    <%= link_to "Pagination" %>
  </div>
</div>
